# Example environment variables for local development
# Set the front-end to use the local backend auth service
REACT_APP_API_URL=http://localhost:5001

# Menu Configuration
# Enable/disable dynamic menu system (loads from database via API)
REACT_APP_USE_DYNAMIC_MENU=false

# Enable/disable fallback to static menu when dynamic menu fails
REACT_APP_FALLBACK_TO_STATIC=true

# Other example variables (add as needed):
# REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
# REACT_APP_SOME_OTHER_KEY=some_value
